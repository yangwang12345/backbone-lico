<div class="modal fade" id="jobModify" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
	<div class="modal-dialog">
		<div class="modal-content" style="width:800px">
			<div class="modal-header job-heading">
				<button id="cancel_modify_x" type="button" class="close" data-dismiss="modal" aria-hidden="true">x
        		</button>
        		<h4 class="modal-title" id="jobSubmitConfirmLabel" style="font-weight:bold">{{nls.edit_job}}
        		</h4>
			</div>
			<form class="form-horizontal job-form-group-margin-left" role="form">
				<div class="form-group job-form-group-margin-left" style="margin-top:15px">
					<label class="col-sm-2 control-label job-font-weight-normal" for="modify_job_name">*{{nls.job_name}}</label>
					<div class="col-sm-10">
						<input style="width:250px;margin-left:3px" class="form-control" type="text" id="modify_job_name" placeholder="Edit hint text in properties panel.">
					</div>
				</div>
				<div class="form-group" style="margin-top:15px">
					<label class="col-sm-2 control-label job-font-weight-normal" for="modify_file_name">*{{nls.job_file}}</label>
					<div class="col-sm-10">
						<input style="width:250px;margin-left:3px" class="form-control" type="text" id="modify_file_name" placeholder="Edit hint text in properties panel." readonly>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label job-font-weight-normal" for="modify_queue_name">*{{nls.queue_name}}</label>
					<select class="form-control" id="modify_queue_name" style="width:110px;margin-left:152px">
						<option value="empty">{{nls.please_select}}</option>
					</select>
				</div>
				<div class="form-group" style="display:none">
					<label class="col-sm-2 control-label" for="modify_work_dir">*{{nls.working_dir}}</label>
					<input style="width:250px;margin-left:152px;position:fixed" class="form-control" type="text" id="modify_work_dir" placeholder="Edit hint text in properties panel.">
					<button style="margin-left:300px;position:fixed" type="button" id="modify_work_dir_select_btn" class="btn btn-primary">{{nls.browse}}
					</button>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label" for="modify_output_dir" style="font-weight:normal;margin-top:-10px">*{{nls.output_path}}<span style="font-size:small;font-weight:normal">({{nls.std_input_output}})</span></label>
					<input style="width:250px;margin-left:152px;position:fixed" class="form-control" type="text" id="modify_output_dir_text" placeholder="Edit hint text in properties panel.">
					<button style="margin-left:300px;position:fixed" type="button" id="modify_output_dir_select_btn" class="btn btn-primary">{{nls.browse}}
					</button>
				</div>
				<div class="form-group" style="display:none">
					<label class="col-sm-2 control-label" for="modify_MPI_prog">*{{nls.MPI_prog}}</label>
					<input style="width:250px;margin-left:144px;position:fixed" class="form-control" type="text" id="modify_MPI_prog_text" placeholder="Edit hint text in properties panel.">
					<button style="margin-left:300px;position:fixed" type="button" id="modify_MPI_prog_select_btn" class="btn btn-primary">{{nls.browse}}
					</button>
				</div>
					<label class="col-sm-2 control-label" style="font:bold">{{nls.res_req}}</label>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label job-font-weight-normal">*{{nls.CPU_core_num}}</label>
					<input style="width:50px;position:fixed;margin-left:152px" class="form-control" type="text" id="modify_CPU_core_num_text">
					<div class="control-label comment_font_color" style="margin-left:210px;position:fixed">{{nls.core}}/div>
					<label class="col-sm-2 control-label job-font-weight-normal" style="margin-left:190px;position:fixed">*{{nls.nodes_num}}</label>
					<input style="width:50px;position:fixed;margin-left:440px" class="form-control" type="text" id="modify_nodes_num">
					<div class="control-label comment_font_color" style="margin-left:500px;position:fixed">{{nls.available}}</div>
					<input style="margin-left:480px;margin-top:9px" type="radio" name="modify_nodesRadios" id="modify_nodesRadios1" value="nodesRadiosOption1">{{nls.share}}
					<input type="radio" name="modify_nodesRadios" id="modify_nodesRadios2" value="nodesRadiosOption2">{{nls.exclusive}}
				</div>
				<div class="form-group">						
					<label class="col-sm-2 control-label job-font-weight-normal">{{nls.mem_size}}</label>
					<input style="width:50px;position:fixed;margin-left:152px" class="form-control" type="text" id="modify_mem_size_text">
					<div class="control-label comment_font_color" style="margin-left:210px;position:fixed">{{nls.giga}}</div>
					<label style="font-size:small;margin-left:157px;margin-top:-8px" class="col-sm-2 control-label job-font-weight-normal">*{{nls.walltime}} (Wall Time)</label>
					<input style="width:130px;position:fixed;margin-left:440px" class="form-control" type="text" id="modify_run_time_text" placeholder="Edit hint text in properties panel.">
					<div class="control-label comment_font_color" style="margin-left:580px;position:absolute">{{nls.hours}}</div>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label" style="font:bold">{{nls.status_notify}}</label>
					<div class="checkbox" style="margin-left:152px">
						<label>
							<input id="modify_notice_finish" type="checkbox" value="finish">{{nls.finish}}
						</label>
						<label>
							<input id="modify_notice_suspend" type="checkbox" value="suspend">{{nls.suspend}}
						</label>
					</div>
				</div>
				<div class="form-group" style="margin-bottom:22px">
					<label class="col-sm-2 control-label job-font-weight-normal" for="modify_email">{{nls.mail}}</label>
					<input style="width:250px;margin-left:152px;position:fixed" class="form-control" type="text" id="modify_email" placeholder="Edit hint text in properties panel.">
					</button>
				</div>
				<div class="form-group" style="margin-bottom:22px">
					<label class="col-sm-2 control-label job-font-weight-normal" for="modify_cell">{{nls.phone}}</label>
					<input style="width:250px;margin-left:152px;position:fixed" class="form-control" type="text" id="modify_cell" placeholder="Edit hint text in properties panel.">
					</button>
				</div>
				<div style="margin-bottom:30px" class="form-group">
					<label class="col-sm-2 control-label" for="confirm_exec_command">*{{nls.exec_command}}</label>
					<textarea style="width:500px;margin-left:152px" class="form-control" rows="9" id="modify_exec_command"></textarea>
				</div>
				
         		<div class="modal-footer">
            		<button type="button" class="btn btn-primary">{{nls.confirm}}
            		</button>
            		<button type="button" class="btn btn-default" data-dismiss="modal">{{nls.cancel}}
            		</button>
     			</div>
				
			</form>
		</div>
	</div>
</div>

