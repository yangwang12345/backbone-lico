{{!第四版布局节点详细信息开始}}
<div class="modal" id="fourth_mymodal">
    <div class="modal-dialog">
        <div class="modal-content" style="width:720px;height:720px;position:relative;left:-80px;">
            <div class="modal-header" style="border-bottom:0px solid #ffffff;">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title"></h4>
            </div>
<div class="modal-body" id="monitor_body_total"   style="padding-top:0px;padding-bottom:0px;">
{{!***********************************************************************************************************************}}
<div id="monitor_body_up" style="width:100%;height:190px;border:0px solid #0000ff;">
</div>

<div id="monitor_body_down" style="width:100%;height:500px;border:0px solid #00ff00;margin-top:20px;">


<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default" style="margin-top:0px">
            <div class="panel-heading">
                <a style=" font-size:16px;color: rgb(0, 0, 0);" id="monitor_history_btn" href="javascript:void(0)"><span>{{nls.systerm_monitor}}</span>&nbsp;</a>&nbsp;&nbsp;&nbsp;
                <span>|</span>
                &nbsp;&nbsp;&nbsp;<a style=" font-size:16px;color: rgb(51, 122, 183);" id="monitor_job_btn" href="javascript:void(0)"><span>{{nls.running_jobs}}</span>&nbsp;</a>
                &nbsp;&nbsp;&nbsp;
                <span>|</span>
                &nbsp;&nbsp;&nbsp;<a style=" font-size:16px;color: rgb(51, 122, 183);"
                                     id="monitor_alarm_btn"
                                     href="javascript:void(0)"><span>{{nls.alarm_btn}}</span>&nbsp;</a>
        
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body" id="history-tab-panel" hide="" style="display:block;height:420px;border:0px solid #ff0000;padding:0px;">
                     <div class="monitor_history_period" style="width:686px;height:25px;margin-top:10px;padding-left:8px;border:0px solid #00ff00;">
                                                <div class="monitor_the_time_of_choice_title" style="line-height:25px;font-weight:100;">{{nls.please_select_period_of_time}}</div>
                                                <div class="monitor_the_time_of_choice_body">
                                                   <select id="monitor_the_time_of_choice" >
                                                     <option data-period="past_hour" value="hour">{{nls.past_one_hour}}</option>
                                                     <option data-period="past_day" value="day">{{nls.past_one_day}}</option>
                                                     <option data-period="past_week" value="week">{{nls.past_one_week}}</option>
                                                     <option data-period="past_month" value="month">{{nls.past_one_month}}</option>
                                                   </select>
                                                </div> 
                    </div>
                   <div id="history-tab-panel_up" style="width:686px;height:115px;border:0px solid #00ff00;"></div>
                   <div id="history-tab-panel_down" style="width:686px;height:260px;border:0px solid #0000ff;"></div>
                   <div id="history-tab-panel_running_jobs"  style="height:420px;width:686px;overflow-x:hidden;overflow-y:auto;display:none;"></div>
                <div id="nodes_alarm_tab_content"
                     style="height:420px;width:686px;overflow-x:hidden;overflow-y:auto;;display:none;">
                    <table id="nodeslarmTable" class="table table-striped table-hover table-bordered dataTable no-footer" width="100%"></table>
                </div>
            </div>
            
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /#page-wrapper -->


</div>


{{!***********************************************************************************************************************}}            

</div>
            
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
{{!第四版布局节点详细信息结束}}


