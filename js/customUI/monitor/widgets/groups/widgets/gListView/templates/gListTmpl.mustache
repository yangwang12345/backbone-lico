<!-- 单个节点关机摸态框 -->
	<div class="modal fade" id="g_powoff_modal" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="true">
		<div class="modal-dialog modal-overall">
			<div class="modal-content modal-content-overall">
				<div class="modal-header modal-header-overall">
					<h4 class="modal-title">{{nls.sure_off}}</h4>
				</div>
				<div class="modal-body">
					<div class="row pl30">
						<div class="col-sm-12">
							<div class="tips-content">
								<div id="g_off_id"></div>
							</div>
						</div>
					</div>
				</div>
				<div class="modal-footer modal-footer-overall">
					<div class="row">
						<div class="col-sm-6">
							<button class="btn btn-primary btn-narrowModal" id="confirmPowerOff">{{nls.confirm}}</button>
						</div>
						<div class="col-sm-6">
							<button class="btn btn-default btn-narrowModal" data-dismiss="modal">{{nls.cancel}}</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
