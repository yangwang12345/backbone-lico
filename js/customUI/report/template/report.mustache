			  <li class="list-group-item part_panel_icon_outter" name="user_loginReportStat" style="border-bottom:1px solid #ffffff"><div class="part_panel_icon"></div>{{nls.user_loginReportStat}}</li>
			  <li class="list-group-item part_panel_icon_outter" name="user_storageReportStat" style="border-bottom:1px solid #ffffff"><div class="part_panel_icon"></div>{{nls.user_storageReportStat}}</li>
