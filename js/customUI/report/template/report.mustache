	    <div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
	      <div class="panel-body">
	        <!--<ul class="list-group">-->
			  <li class="list-group-item part_panel_icon_outter" name="logReportDetail" style="border-bottom:1px solid #ffffff"><div class="part_panel_icon"></div>{{nls.logReportDetail}}</li>
			  <li class="list-group-item part_panel_icon_outter" name="node_runningReportStat" style="border-bottom:1px solid #ffffff"><div class="part_panel_icon"></div>{{nls.node_runningReportStat}}</li>
