<div id="reportView">
	<div class="jobs_select_templates">
	<header class="jobs_select_templates_choice" style="font-family: '微软雅黑' !important;">{{nls.listHeader}}</header>
	<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
