<div class="modal fade" id="session_deleteFinishConfirm" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
	<div class="modal-dialog">	
		<div class="modal-content  modal-content_session" >
			<div class="modal-header">
        		<h4 class="modal-title modal-title_session"  id="sessionDeleteFinishLabel">{{nls.delete_finish_session_confirm}}
        		</h4>
			</div>
			<div class="modal-body">
				<div id="delete_finish_sessionName_section">
				</div>
				<div id="delete_finish_hostName_section" >
				</div>
				<div id="delete_finish_owner_section" >
				</div>
         	</div>
     		<div class="modal-footer">
        		<button id="session_deleteFinishConfirm_btn" type="button" class="btn btn-primary">{{nls.confirm}}
        		</button>
        		<button type="button" class="btn btn-default btn-default_session" data-dismiss="modal" >{{nls.cancel}}
        		</button>
 			</div>
		</div>
	</div>
</div>

