		<div style="width:475px;margin-left:+35px">
			<div id ="sessionCancelResultArea">
				<img id="sessionCancelIcon" style="position:fixed;margin-top:-9px"></img>
				<div id="sessionCancelResultText" style="margin-left:45px"></div>
				<button style="margin-top:-20px" type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
     		</div>
		</div>
	</div>
</div>


