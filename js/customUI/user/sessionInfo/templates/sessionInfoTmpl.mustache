<div class="modal fade" id="sessionInfo_deleteFinishConfirm" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
	<div class="modal-dialog">	
		<div class="modal-content" style="width:450px">
			<div class="modal-header">
        		<h4 class="modal-title" style="margin-left:-60px;margin-top:5px" id="sessionInfoDeleteFinishLabel">{{nls.delete_finish_session_confirm}}
        		</h4>
			</div>
			<div class="modal-body">
				<div id="delete_finish_sessionName_section" style="margin-left:15px;font-size:14px;margin-bottom:5px">
				</div>
				<div id="delete_finish_hostName_section" style="margin-left:15px;font-size:14px;margin-bottom:5px">
				</div>
				<div id="delete_finish_owner_section" style="margin-left:15px;font-size:14px;margin-bottom:5px">
				</div>
         	</div>
     		<div class="modal-footer">
        		<button id="sessionInfo_deleteFinishConfirm_btn" type="button" class="btn btn-primary" style="float:left;margin-left:16px;margin-bottom:20px;width:100px">{{nls.confirm}}
        		</button>
        		<button type="button" class="btn btn-default" data-dismiss="modal" style="float:left;margin-left:40px;margin-bottom:20px;width:100px;background-color:#eee">{{nls.cancel}}
        		</button>
 			</div>
		</div>
	</div>
</div>



